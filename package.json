  },
  "devDependencies": {
    "@types/keccak": "^3.0.1",
    "@types/node": "^18.0.3",
    "esbuild": "^0.14.49",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
